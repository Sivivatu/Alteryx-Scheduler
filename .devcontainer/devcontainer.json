{
    "name": "Ayx-Scheduler",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/Alteryx-Scheduler",
    // "forwardPorts": [5432],
    "postStartCommand": "pipenv install", // && pipenv shell", //pipenv install --system && sudo chown -R vscode:vscode /workspaces/Alteryx-Scheduler && 
    "customizations": {
      "vscode": {
        "extensions": [
          "patbenatar.advanced-new-file",
          "ms-python.python",
          // "ms-python.vscode-pylance",
          "GitHub.copilot-chat",
          "GitHub.copilot",
          "ms-azuretools.vscode-docker",
          "bradlc.vscode-tailwindcss",
          "alexcvzz.vscode-sqlite",
          "charliermarsh.ruff",
